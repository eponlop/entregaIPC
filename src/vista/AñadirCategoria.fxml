<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlador.AñadirCategoriaController">
   <children>
      <HBox maxHeight="1.7976931348623157E308" VBox.vgrow="ALWAYS" />
      <VBox prefHeight="70.0" VBox.vgrow="NEVER">
         <children>
            <Label text="Nombre de la categoria">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </Label>
            <TextField maxWidth="-Infinity" prefHeight="26.0" prefWidth="186.0" />
         </children>
      </VBox>
      <VBox prefHeight="70.0" VBox.vgrow="NEVER">
         <children>
            <Label text="Descripción de la categoria">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </Label>
            <TextField maxHeight="-Infinity" maxWidth="-Infinity" prefWidth="400.0" />
         </children>
      </VBox>
      <HBox maxHeight="1.7976931348623157E308" VBox.vgrow="ALWAYS" />
      <HBox alignment="BOTTOM_RIGHT" maxHeight="-Infinity" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button mnemonicParsing="false" text="Aceptar" />
            <Button fx:id="cancelarButton" mnemonicParsing="false" onMouseClicked="#cancelar" text="Cancelar">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="40.0" right="10.0" top="10.0" />
   </padding>
</VBox>
