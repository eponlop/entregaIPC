<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox alignment="CENTER" stylesheets="@../resources/stylesheets/HojaEstilo.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlador.RegistroUsuarioController">
   <children>
      <VBox maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
      <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" stylesheets="@../resources/stylesheets/HojaEstilo.css">
         <children>
            <HBox maxHeight="1.7976931348623157E308" VBox.vgrow="ALWAYS" />
            <HBox VBox.vgrow="NEVER">
               <children>
                  <VBox maxHeight="-Infinity" prefHeight="70.0">
                     <children>
                        <Label style="-fx-text-fill: #489FEA;" styleClass="texto-head" text="Nombre" textFill="#ff6363">
                           <VBox.margin>
                              <Insets bottom="5.0" />
                           </VBox.margin>
                        </Label>
                        <TextField fx:id="nombreText" maxWidth="-Infinity" prefHeight="26.0" prefWidth="130.0" />
                     </children>
                     <HBox.margin>
                        <Insets right="75.0" />
                     </HBox.margin>
                  </VBox>
                  <VBox maxHeight="-Infinity" prefHeight="70.0">
                     <children>
                        <Label style="-fx-text-fill: #489FEA;" styleClass="texto-head" text="Primer apellido" textFill="#ff6363">
                           <VBox.margin>
                              <Insets bottom="5.0" />
                           </VBox.margin>
                        </Label>
                        <TextField fx:id="apellidoText" maxWidth="-Infinity" prefHeight="26.0" prefWidth="130.0" />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <VBox maxHeight="-Infinity" prefHeight="70.0" VBox.vgrow="NEVER">
               <children>
                  <Label style="-fx-text-fill: #489FEA;" styleClass="texto-head" text="Correo electronico" textFill="#ff6363">
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                  </Label>
                  <TextField fx:id="emailText" maxWidth="-Infinity" prefHeight="26.0" prefWidth="220.0" />
               </children>
            </VBox>
            <VBox maxHeight="-Infinity" prefHeight="70.0">
               <children>
                  <Label style="-fx-text-fill: #489FEA;" styleClass="texto-head" text="Nickname" textFill="#ff6363">
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                  </Label>
                  <TextField fx:id="loginText" maxWidth="-Infinity" prefHeight="27.0" prefWidth="115.0" />
               </children>
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </VBox>
            <HBox>
               <children>
                  <VBox maxHeight="-Infinity" prefHeight="70.0">
                     <children>
                        <Label style="-fx-text-fill: #489FEA;" styleClass="texto-head" text="Contraseña" textFill="#ff6363">
                           <VBox.margin>
                              <Insets bottom="5.0" />
                           </VBox.margin>
                        </Label>
                        <TextField fx:id="passwordText" maxWidth="-Infinity" prefHeight="26.0" prefWidth="200.0" />
                     </children>
                     <HBox.margin>
                        <Insets right="75.0" />
                     </HBox.margin>
                  </VBox>
                  <VBox maxHeight="-Infinity" prefHeight="70.0">
                     <children>
                        <Label style="-fx-text-fill: #489FEA;" styleClass="texto-head" text="Repite tu contraseña" textFill="#ff6363">
                           <VBox.margin>
                              <Insets bottom="5.0" />
                           </VBox.margin>
                        </Label>
                        <TextField fx:id="repeatPassText" maxWidth="-Infinity" prefHeight="26.0" prefWidth="200.0" />
                     </children>
                     <HBox.margin>
                        <Insets right="30.0" />
                     </HBox.margin>
                  </VBox>
               </children>
            </HBox>
            <HBox>
               <children>
                  <VBox maxHeight="-Infinity">
                     <children>
                        <Label style="-fx-text-fill: #489FEA;" styleClass="texto-head" text="Foto de perfil (opcional)" textFill="#ff6363">
                           <VBox.margin>
                              <Insets bottom="5.0" />
                           </VBox.margin>
                        </Label>
                        <Button mnemonicParsing="false" onMouseClicked="#seleccionarArchivo" style="-fx-pref-width: 100pt;" styleClass="button-bold" text="Seleccionar">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </Button>
                     </children>
                  </VBox>
                  <ImageView fx:id="imageView" fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true">
                     <HBox.margin>
                        <Insets left="60.0" />
                     </HBox.margin>
                  </ImageView>
               </children>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="BOTTOM_LEFT" maxHeight="-Infinity" prefHeight="46.0" prefWidth="600.0">
               <children>
                  <HBox prefWidth="200.0" />
                  <Button mnemonicParsing="false" onMouseClicked="#aceptar" style="-fx-pref-width: 100pt;" styleClass="button-bold" text="Aceptar">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="cancelarButton" mnemonicParsing="false" onMouseClicked="#cancelar" style="-fx-pref-width: 100pt;" styleClass="button-normal" text="Cancelar">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets bottom="10.0" right="30.0" />
               </VBox.margin>
            </HBox>
            <HBox maxHeight="1.7976931348623157E308" VBox.vgrow="ALWAYS" />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets bottom="10.0" left="40.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
      <VBox maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
   </children>
</HBox>
