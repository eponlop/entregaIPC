<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="BOTTOM_LEFT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" stylesheets="@../resources/stylesheets/HojaEstilo.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlador.ConfiguracionPerfilController">
   <children>
      <HBox maxHeight="1.7976931348623157E308" VBox.vgrow="ALWAYS" />
      <HBox VBox.vgrow="NEVER">
         <children>
            <VBox maxHeight="-Infinity" prefHeight="70.0">
               <children>
                  <Label styleClass="texto-head" text="Nombre" textFill="#ff6161">
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                  </Label>
                  <TextField editable="false" maxWidth="-Infinity" prefHeight="26.0" prefWidth="190.0" />
               </children>
               <HBox.margin>
                  <Insets right="75.0" />
               </HBox.margin>
            </VBox>
            <VBox maxHeight="-Infinity" prefHeight="70.0">
               <children>
                  <Label styleClass="texto-head" text="Primer apellido" textFill="#ff6161">
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                  </Label>
                  <TextField editable="false" maxWidth="-Infinity" prefHeight="26.0" prefWidth="190.0" />
               </children>
            </VBox>
         </children>
      </HBox>
      <VBox maxHeight="-Infinity" prefHeight="70.0" VBox.vgrow="NEVER">
         <children>
            <Label styleClass="texto-head" text="Correo electronico" textFill="#ff6161">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </Label>
            <TextField editable="false" maxWidth="-Infinity" prefHeight="26.0" prefWidth="190.0" />
         </children>
      </VBox>
      <HBox maxHeight="1.7976931348623157E308" VBox.vgrow="ALWAYS" />
      <HBox>
         <children>
            <VBox maxHeight="-Infinity" prefHeight="70.0">
               <children>
                  <Label styleClass="texto-head" text="Nueva contraseña" textFill="#ff6161">
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                  </Label>
                  <TextField editable="false" maxWidth="-Infinity" prefHeight="26.0" prefWidth="290.0" />
               </children>
               <HBox.margin>
                  <Insets right="75.0" />
               </HBox.margin>
            </VBox>
            <VBox maxHeight="-Infinity" prefHeight="70.0">
               <children>
                  <Label styleClass="texto-head" text="Repite tu nueva contraseña" textFill="#ff6161">
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                  </Label>
                  <TextField editable="false" maxWidth="-Infinity" prefHeight="26.0" prefWidth="290.0" />
               </children>
               <HBox.margin>
                  <Insets right="30.0" />
               </HBox.margin>
            </VBox>
         </children>
      </HBox>
      <HBox maxHeight="1.7976931348623157E308" VBox.vgrow="ALWAYS" />
      <HBox>
         <children>
            <VBox maxHeight="-Infinity">
               <children>
                  <Label styleClass="texto-head" text="Cambiar foto de perfil" textFill="#ff6161">
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                  </Label>
                  <Button disable="true" mnemonicParsing="false" styleClass="button-normal" text="Seleccionar">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
            <ImageView fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true" visible="false">
               <HBox.margin>
                  <Insets left="50.0" />
               </HBox.margin>
            </ImageView>
         </children>
      </HBox>
      <HBox alignment="BOTTOM_RIGHT" maxHeight="-Infinity" prefHeight="46.0" prefWidth="600.0">
         <children>
            <StackPane fx:id="stackPane" alignment="BOTTOM_CENTER">
               <children>
                  <HBox fx:id="cajaDelStack" alignment="BOTTOM_LEFT">
                     <children>
                        <Button fx:id="guardarButton" disable="true" mnemonicParsing="false" onMouseClicked="#guardarCambiosClick" prefHeight="28.0" prefWidth="155.0" styleClass="button-bold" text="Guardar cambios">
                           <HBox.margin>
                              <Insets left="20.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="cancelarButton" disable="true" mnemonicParsing="false" onMouseClicked="#cancelarClick" styleClass="button-normal" text="Cancelar">
                           <opaqueInsets>
                              <Insets />
                           </opaqueInsets>
                           <HBox.margin>
                              <Insets left="20.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                  </HBox>
                  <Button fx:id="editarButton" mnemonicParsing="false" onMouseClicked="#editarClick" prefHeight="28.0" prefWidth="155.0" styleClass="button-bold" text="Editar" />
               </children>
            </StackPane>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" right="30.0" />
         </VBox.margin>
      </HBox>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="10.0" left="40.0" right="10.0" top="10.0" />
   </padding>
</VBox>
